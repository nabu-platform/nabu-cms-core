    <broadcastCreate>false</broadcastCreate>
    <broadcastUpdate>false</broadcastUpdate>
    <coreType>nabu.cms.core.types.emodel.core.Node</coreType>
    <createRole>$user</createRole>
    <deleteRole>$user</deleteRole>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>component</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>id</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>name</key>
        <operator>ilike</operator>
        <or>false</or>
        <alias>q</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>description</key>
        <or>true</or>
        <alias>q</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>title</key>
        <or>true</or>
        <alias>q</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>ownerId</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>parentId</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <foreignFields>
        <foreignName>componentId:name</foreignName>
        <localName>component</localName>
    </foreignFields>
    <listRole>$user</listRole>
    <provider>nabu.services.crud.provider.basic.provider</provider>
    <updateRole>$user</updateRole>
    <useExplicitLanguage>false</useExplicitLanguage>
    <useLanguage>false</useLanguage>
    <useListOutputForCreate>false</useListOutputForCreate>
    <useListOutputForUpdate>false</useListOutputForUpdate>
</crud>
