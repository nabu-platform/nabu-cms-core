<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<oAuth2>
    <scopes>email</scopes>
    <scopes>profile</scopes>
    <loginEndpoint>https://accounts.google.com/o/oauth2/auth</loginEndpoint>
    <tokenEndpoint>https://www.googleapis.com/oauth2/v3/token</tokenEndpoint>
    <serverPath>oauth2/google</serverPath>
    <errorPath>oauth2/error</errorPath>
    <successPath>/</successPath>
    <authenticatorService>nabu.cms.core.providers.oauth2.oauth2Authenticator</authenticatorService>
    <requireStateToken>false</requireStateToken>
    <redirectUriInTokenRequest>true</redirectUriInTokenRequest>
    <jwtUseOriginalRealm>false</jwtUseOriginalRealm>
</oAuth2>
